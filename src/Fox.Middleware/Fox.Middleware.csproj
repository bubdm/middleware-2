<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <PropertyGroup>
    <Version>0.0.1</Version>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>Fox.Middleware</PackageId>
    <PackageTitle>Fox.Middleware</PackageTitle>
    <PackageTags>middleware</PackageTags>
    <License>http://opensource.org/licenses/Apache-2.0</License>
    <PackageProjectUrl>https://github.com/fox-framework/middleware</PackageProjectUrl>
    <RepositoryUrl>https://github.com/fox-framework/middleware</RepositoryUrl>
    <PackageReleaseNotes>This is a work in progress</PackageReleaseNotes>
    <IncludeSymbols>true</IncludeSymbols>
    <IncludeSource>false</IncludeSource>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="2.2.0" />
  </ItemGroup>

</Project>
